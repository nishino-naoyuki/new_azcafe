<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="jp.ac.asojuku.azcafe.entity">

	<class name="CourseTblEntity" table="COURSE_TBL">

		<id name="courseId" type="java.lang.Integer" column="course_id">
			<generator class="assigned" />
		</id>

		<property name="name" type="java.lang.String"
			column="name" not-null="true" />

		<property name="year" type="java.lang.Integer"
			column="year" not-null="true" />

	</class>
</hibernate-mapping>
