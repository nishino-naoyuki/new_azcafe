<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="jp.ac.asojuku.azcafe.entity">

	<class name="SkillAssTblEntity" table="SKILL_ASS_TBL">

		<id name="skillAssId" type="java.lang.Integer" column="skill_ass_id">
			<generator class="assigned" />
		</id>

		<property name="assignmentId" type="java.lang.Integer"
			column="assignment_id" not-null="true" />

		<property name="skillId" type="java.lang.Integer"
			column="skill_id" not-null="true" />

	</class>
</hibernate-mapping>
