<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="jp.ac.asojuku.azcafe.entity">

	<class name="AnswerDetailTblEntity" table="ANSWER_DETAIL_TBL">

		<id name="answerDetailId" type="java.lang.Integer" column="answer_detail_id">
			<generator class="assigned" />
		</id>

		<property name="answerId" type="java.lang.Integer"
			column="answer_id" not-null="true" />

		<property name="filename" type="java.lang.String"
			column="filename" not-null="true" />

		<property name="answer" type="java.lang.String"
			column="answer" not-null="true" />

	</class>
</hibernate-mapping>
