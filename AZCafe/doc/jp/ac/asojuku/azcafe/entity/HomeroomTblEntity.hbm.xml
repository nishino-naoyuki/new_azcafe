<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="jp.ac.asojuku.azcafe.entity">

	<class name="HomeroomTblEntity" table="HOMEROOM_TBL">

		<id name="homeroomId" type="java.lang.Integer" column="homeroom_id">
			<generator class="assigned" />
		</id>

		<property name="courseId" type="java.lang.Integer"
			column="course_id" not-null="true" />

		<property name="name" type="java.lang.String"
			column="name" not-null="false" />

	</class>
</hibernate-mapping>
